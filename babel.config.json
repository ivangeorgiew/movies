{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",
                "bugFixes": true,
                "corejs": { "version": 3, "proposals": true }
            }
        ],
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "plugins": ["@babel/plugin-syntax-import-meta"],
    "env": {
        "development": { "plugins": ["react-refresh/babel"] }
    }
}
